<%= render partial: '/concerts/errors' %>

   <%= form_for :session, url: sessions_path do |f| %>
  
   <p>
   	<%= f.label "Email" %><br>
   	<%= f.text_field :email %>
   </p>

   <p>
   	<%= f.label "Password" %><br>
   	<%= f.password_field :password %>
   </p>

   <p>
   	<%= f.submit %>
   </p>
   <% end %> 