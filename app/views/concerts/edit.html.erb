
<div class="edit-concert">
<%= form_for :concert, url: concert_path(@concert), method: :patch do |f|%>
	<p>
		<%= f.label :band %> <br>
		<%= f.text_field :band %>
	</p>

	<p>
		<%= f.label :venue %> <br>
		<%= f.text_field :venue %>
	</p>	

	<p>
		<%= f.label :date %> <br>
		<%= f.date_field :date %>
	</p>	

	<p>
		<%= f.label :time %> <br>
		<%= f.time_field :start_time %>
	</p>	

	<p>
		<%=f.submit %> 
	</p>

	<% end  %> 

</div>